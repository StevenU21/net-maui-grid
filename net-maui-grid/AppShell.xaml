<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="net_maui_grid.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:net_maui_grid"
    xmlns:pages="clr-namespace:net_maui_grid.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="net_maui_grid">

    <FlyoutItem Title="Login">
        <ShellContent ContentTemplate="{DataTemplate pages:Login}" />
    </FlyoutItem>

    <FlyoutItem Title="Calculadora">
        <ShellContent ContentTemplate="{DataTemplate pages:Calculadora}" />
    </FlyoutItem>

    <FlyoutItem Title="Formulario">
        <ShellContent ContentTemplate="{DataTemplate pages:Formulario}" />
    </FlyoutItem>

    <FlyoutItem Title="Registro">
        <ShellContent ContentTemplate="{DataTemplate pages:Registro}" />
    </FlyoutItem>

</Shell>
